{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
   
    "rewrites": [
      { "source": "/(.*)", "destination": "dist/server.js" }
    ],
    
      "headers": [
        {
          "source": "/socket.io/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "https://electon-one.vercel.app" },
            { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
            { "key": "Access-Control-Allow-Credentials", "value": "true" }
          ]
        }
      ]
    
    
  }